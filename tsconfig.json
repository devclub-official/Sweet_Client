{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@/components": ["src/components/*"],
      "@/navigation": ["src/navigation/*"],
      "@/screens": ["src/screens/*"],
      "@/assets": ["src/assets/*"],
      "@/libs": ["src/libs/*"],
      "@/types": ["src/types/*"],
      "@/utils": ["src/utils/*"],
      "@/apis": ["src/apis/*"],
      "@/stores": ["src/stores/*"],
      "@/hooks": ["src/hooks/*"]
    }
  }
}
